@model List<Product>
<section class="section" id="social">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-heading">
                    <h2>Son Eklenen Ürünler</h2>
                    
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row images">
            @foreach (var item in Model)
            {
                <div class="col-2">
                    <div class="thumb" style="margin-right:10px">
                        <img src="@item.ProductImageCover" alt="">
                       
                    </div>
                </div>
            }

        </div>
    </div>
</section>
